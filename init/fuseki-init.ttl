# RDF initialization script for Apache Jena Fuseki
# E-commerce Platform using RDF/OWL vocabulary

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/ecommerce#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .

# ========== DEFINE CLASSES ==========
ex:User a owl:Class ;
    rdfs:label "User" ;
    rdfs:comment "A user in the e-commerce platform" .

ex:Product a owl:Class ;
    rdfs:label "Product" ;
    rdfs:comment "A product sold in the platform" .

ex:Order a owl:Class ;
    rdfs:label "Order" ;
    rdfs:comment "An order placed by a user" .

ex:OrderItem a owl:Class ;
    rdfs:label "OrderItem" ;
    rdfs:comment "An item within an order" .

ex:Review a owl:Class ;
    rdfs:label "Review" ;
    rdfs:comment "A product review" .

ex:Category a owl:Class ;
    rdfs:label "Category" ;
    rdfs:comment "A product category" .

ex:Payment a owl:Class ;
    rdfs:label "Payment" ;
    rdfs:comment "A payment transaction" .

ex:CartItem a owl:Class ;
    rdfs:label "CartItem" ;
    rdfs:comment "An item in shopping cart" .

# ========== DEFINE PROPERTIES ==========
ex:hasEmail a owl:DatatypeProperty ;
    rdfs:domain ex:User ;
    rdfs:range xsd:string .

ex:hasAge a owl:DatatypeProperty ;
    rdfs:domain ex:User ;
    rdfs:range xsd:integer .

ex:hasCity a owl:DatatypeProperty ;
    rdfs:domain ex:User ;
    rdfs:range xsd:string .

ex:hasRole a owl:DatatypeProperty ;
    rdfs:domain ex:User ;
    rdfs:range xsd:string .

ex:hasPhone a owl:DatatypeProperty ;
    rdfs:domain ex:User ;
    rdfs:range xsd:string .

ex:hasSKU a owl:DatatypeProperty ;
    rdfs:domain ex:Product ;
    rdfs:range xsd:string .

ex:hasPrice a owl:DatatypeProperty ;
    rdfs:domain ex:Product ;
    rdfs:range xsd:decimal .

ex:hasStock a owl:DatatypeProperty ;
    rdfs:domain ex:Product ;
    rdfs:range xsd:integer .

ex:hasRating a owl:DatatypeProperty ;
    rdfs:domain ex:Product ;
    rdfs:range xsd:decimal .

ex:hasTotalAmount a owl:DatatypeProperty ;
    rdfs:domain ex:Order ;
    rdfs:range xsd:decimal .

ex:hasStatus a owl:DatatypeProperty ;
    rdfs:domain ex:Order ;
    rdfs:range xsd:string .

ex:hasQuantity a owl:DatatypeProperty ;
    rdfs:domain ex:OrderItem ;
    rdfs:range xsd:integer .

ex:hasReviewRating a owl:DatatypeProperty ;
    rdfs:domain ex:Review ;
    rdfs:range xsd:integer .

ex:hasAmount a owl:DatatypeProperty ;
    rdfs:domain ex:Payment ;
    rdfs:range xsd:decimal .

ex:hasMethod a owl:DatatypeProperty ;
    rdfs:domain ex:Payment ;
    rdfs:range xsd:string .

ex:placedOrder a owl:ObjectProperty ;
    rdfs:domain ex:User ;
    rdfs:range ex:Order ;
    rdfs:label "placed order" .

ex:containsItem a owl:ObjectProperty ;
    rdfs:domain ex:Order ;
    rdfs:range ex:OrderItem ;
    rdfs:label "contains item" .

ex:forProduct a owl:ObjectProperty ;
    rdfs:domain ex:OrderItem ;
    rdfs:range ex:Product ;
    rdfs:label "for product" .

ex:belongsToCategory a owl:ObjectProperty ;
    rdfs:domain ex:Product ;
    rdfs:range ex:Category ;
    rdfs:label "belongs to category" .

ex:wroteReview a owl:ObjectProperty ;
    rdfs:domain ex:User ;
    rdfs:range ex:Review ;
    rdfs:label "wrote review" .

ex:reviewsProduct a owl:ObjectProperty ;
    rdfs:domain ex:Review ;
    rdfs:range ex:Product ;
    rdfs:label "reviews product" .

ex:hasPayment a owl:ObjectProperty ;
    rdfs:domain ex:Order ;
    rdfs:range ex:Payment ;
    rdfs:label "has payment" .

ex:madePayment a owl:ObjectProperty ;
    rdfs:domain ex:User ;
    rdfs:range ex:Payment ;
    rdfs:label "made payment" .

ex:addedToCart a owl:ObjectProperty ;
    rdfs:domain ex:User ;
    rdfs:range ex:Product ;
    rdfs:label "added to cart" .

ex:follows a owl:ObjectProperty ;
    rdfs:domain ex:User ;
    rdfs:range ex:User ;
    rdfs:label "follows" .

